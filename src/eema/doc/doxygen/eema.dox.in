#---------------------------------------------------------------------------
# Project related configuration options
#---------------------------------------------------------------------------
DOXYFILE_ENCODING      = UTF-8
PROJECT_NAME           = "Embedded Element Method Algorithms (EMMA)" 
PROJECT_BRIEF          = "Finite element code with support for the embedded element method"
PROJECT_NUMBER         = @EEMA_VERSION@
OUTPUT_DIRECTORY       = .
OUTPUT_LANGUAGE        = English
RECURSIVE              = YES
FULL_PATH_NAMES        = YES
STRIP_FROM_PATH        = @CMAKE_SOURCE_DIR@
STRIP_FROM_INC_PATH    = @CMAKE_SOURCE_DIR@
TAB_SIZE               = 4
BUILTIN_STL_SUPPORT    = NO
BRIEF_MEMBER_DESC      = YES
MARKDOWN_SUPPORT       = YES
INPUT                  +=@CMAKE_SOURCE_DIR@/src/eema/doc/resources/mainpage.dox
#USE_MDFILE_AS_MAINPAGE = @CMAKE_SOURCE_DIR@/README.md
REPEAT_BRIEF           = YES
EXTRACT_ALL            = YES
EXTRACT_PRIVATE        = YES
#---------------------------------------------------------------------------
# configuration options related to warning and progress messages
#---------------------------------------------------------------------------
QUIET                  = @DOXYGEN_QUIET@
WARNINGS               = @DOXYGEN_WARNINGS@
WARN_IF_UNDOCUMENTED   = @DOXYGEN_WARNINGS@
WARN_IF_DOC_ERROR      = @DOXYGEN_WARNINGS@
#---------------------------------------------------------------------------
# configuration options related to the input files
#---------------------------------------------------------------------------
INPUT                  += @CMAKE_SOURCE_DIR@/src
PROJECT_LOGO           = @CMAKE_SOURCE_DIR@/src/eema/doc/resources/web-logo.png
#EXAMPLE_PATH           = @CMAKE_SOURCE_DIR@/test
#---------------------------------------------------------------------------
# configuration options related to source browsing
#---------------------------------------------------------------------------
SOURCE_BROWSER         = YES
INLINE_SOURCES         = NO
#---------------------------------------------------------------------------
# Configuration options related to the preprocessor
#---------------------------------------------------------------------------
ENABLE_PREPROCESSING   = YES
MACRO_EXPANSION        = YES
EXPAND_ONLY_PREDEF     = YES
SEARCH_INCLUDES        = YES
INCLUDE_PATH           = @CMAKE_SOURCE_DIR@/src/eema/src/TimeStep \
		         @CMAKE_SOURCE_DIR@/src/eema/src/Math/GeneralMath \
			 @CMAKE_SOURCE_DIR@/src/eema/src/Materials \
			 @CMAKE_SOURCE_DIR@/src/eema/src/Materials/StressUpdate \
			 @CMAKE_SOURCE_DIR@/src/eema/src/Materials/Saint-Venant \
			 @CMAKE_SOURCE_DIR@/src/eema/src/Materials/Ogden-Hyperelastic \
                         @CMAKE_SOURCE_DIR@/src/eema/src/Materials/Mooney-Rivlin-Hyperelastic \
			 @CMAKE_SOURCE_DIR@/src/eema/src/Materials/Elastic \
			 @CMAKE_SOURCE_DIR@/src/eema/src/Input-Output/Input \
			 @CMAKE_SOURCE_DIR@/src/eema/src/Input-Output/Output \
			 @CMAKE_SOURCE_DIR@/src/eema/src/Elements/ShapeFunctions \ 
			 @CMAKE_SOURCE_DIR@/src/eema/src/Elements/Quadrature \ 
			 @CMAKE_SOURCE_DIR@/src/eema/src/Elements/Mass \ 
 			 @CMAKE_SOURCE_DIR@/src/eema/src/Elements/ElementCalculations \ 
			 @CMAKE_SOURCE_DIR@/src/eema/src/FEM/Explicit \ 
 		         @CMAKE_SOURCE_DIR@/src/eema/src/FEM/Electromechanics \ 
 			 @CMAKE_SOURCE_DIR@/src/eema/src/FEM/BoundaryConditions \ 
			 @CMAKE_SOURCE_DIR@/src/eema/src/FEM/Assembly-Global-Local \
                         @CMAKE_SOURCE_DIR@/src/eema/include \
                         @CMAKE_SOURCE_DIR@

INCLUDE_FILE_PATTERNS  = *.h
PREDEFINED             = @EEMA_DOXYGEN_DEFINTIONS@
EXPAND_AS_DEFINED      = __BEGIN_EEMA__ \
                         __END_EEMA__ \
                         EEMA_GET_MACRO \
                         EEMA_SET_MACRO \
                         EEMA_GET_MACRO_NOT_CONST \
                         EEMA_GET_MACRO_BY_ELEMENT_TYPE

SKIP_FUNCTION_MACROS   = YES

#---------------------------------------------------------------------------
# Configuration options related to the dot tool
#---------------------------------------------------------------------------
CLASS_DIAGRAMS         = YES
HAVE_DOT               = YES
CLASS_GRAPH            = YES
COLLABORATION_GRAPH    = YES
TEMPLATE_RELATIONS     = YES
CALL_GRAPH             = YES
CALLER_GRAPH           = YES
DOT_PATH               = @DOXYGEN_DOT_PATH@
DOT_GRAPH_MAX_NODES    = 100

#---------------------------------------------------------------------------
# configuration options related to the man page output
#---------------------------------------------------------------------------
GENERATE_MAN           = YES
MAN_OUTPUT             = man
MAN_EXTENSION          = .3
MAN_LINKS              = NO

#---------------------------------------------------------------------------
# configuration options related to the LaTeX output
#---------------------------------------------------------------------------
GENERATE_LATEX         = YES
LATEX_OUTPUT           = latex
LATEX_CMD_NAME         = latex
MAKEINDEX_CMD_NAME     = makeindex
COMPACT_LATEX          = NO
PAPER_TYPE             = a4
EXTRA_PACKAGES         = 
LATEX_HEADER           = 
LATEX_FOOTER           = 
PDF_HYPERLINKS         = YES
USE_PDFLATEX           = YES
LATEX_BATCHMODE        = NO
LATEX_HIDE_INDICES     = NO
LATEX_SOURCE_CODE      = YES
